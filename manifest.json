{
	"manifest_version": 2,
	"name": "Scribble Pad",
	"version": "1.0.0",
	"description": "modifies the web pages HTML and CSS",
	"web_accessible_resources": [
		"*.png"
	],
	"icons": {
		"128": "icon128.png"
	},
	"browser_action":{
		"default_icon": "icon16.png"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["p5.js", "p5.dom.js", "content.js"]
	}]
}